			<jsitem location="inlinesnippet" name="getNewImage" cachelocation="false">var params = arguments[0],
	$ = skuid.$;

// direction to move - next or previous
var direction = params.$Input.direction;
var images = skuid.$M('App_Images').getRows();
var len = images.length;
var uiImageRow = skuid.$M('Current_Image').getFirstRow();
var curIndex = uiImageRow.Index

var newIndex = 0;
if (direction == "next") {
    newIndex = (curIndex + 1) % len;
} else if (direction == "previous") {
    newIndex = ((curIndex - 1) + len) % len;
}

skuid.$M('Current_Image').updateRow(uiImageRow,
    {
        Index: newIndex,
        Name: images[newIndex].related_image_name,
        Description:images[newIndex].related_image_description,
        URL: images[newIndex].location_url
    }
);
</jsitem>
		</javascript>
