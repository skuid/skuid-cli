			<jsitem location="inlinesnippet" name="checkValidityofInput" cachelocation="false">var params = arguments[0],
	$ = skuid.$;

var row = params.row;
var secureinput = row.input_id;
var sirow = skuid.$M('User_Inputs').getRowById(secureinput);
var encoding = sirow.encoding;


if (encoding === "base64 JSON") {
    var value = row.textareavalue;
    if (value === "") {
        params.model.updateRow(params.row,{validation_status:'blank'});
        return;
    } else {
        try {
            var outputs = JSON.parse(atob(value.trim()));
            checkOutputs(outputs);
        } catch (error) {
            params.model.updateRow(params.row,{validation_status:'invalid'});
            return;
        }
        params.model.updateRow(params.row,{validation_status:'success'});

    }
} else if (encoding == "none" &amp;&amp; row.textvalue !== "") {
    params.model.updateRow(params.row,{validation_status:'success'});
}

function checkOutputs(outputs) {
    var outputsModel = skuid.$M('ExpectedOutputs');
    $.each(outputsModel.getRows(), function(i,row){
        if ( outputs[row.output_name] !== undefined ) {
            outputsModel.updateRow(row, {has_value:true});
        }
    });
}


</jsitem>
			<jsitem location="inlinesnippet" name="setupInputPlaceholders" cachelocation="false">var params = arguments[0],
	$ = skuid.$;
	
var valuesModel = skuid.$M('Values');
var inputsModel = skuid.$M('User_Inputs');

// loop through all inputs, creating a row in the Values model for each input
$.each(inputsModel.getRows(),function(i,row){
    valuesModel.createRow({
        additionalConditions: [
        { field: 'input_id', value: row.id},
        { field: 'type', value: row.type },
        { field: 'input_name', value: row.user_input_name}
    ], doAppend: true
    });
});</jsitem>
		</javascript>
